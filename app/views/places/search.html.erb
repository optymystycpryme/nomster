<div class="container">
  <% @places.each do |place| %>
    <div class="booyah-box">
      <h1><%= place.name %></h1>
      <i><%= place.address %></i><br><br>
      <p><%= place.description %> </p>
    </div>
    <hr>
  <% end %>
  <div class="d-flex justify-content-center">
    <%= will_paginate @places, renderer: WillPaginate::ActionView::BootstrapLinkRenderer %>
  </div>
</div>